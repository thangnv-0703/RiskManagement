{
  "name": "assessment-result",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/assessment-result/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/assessment-result",
        "main": "apps/assessment-result/src/main.ts",
        "tsConfig": "apps/assessment-result/tsconfig.app.json",
        "isolatedConfig": true,
        "webpackConfig": "apps/assessment-result/webpack.config.js",
        "generatePackageJson": true,
        "tsPlugins": [
          {
            "name": "@nestjs/swagger/plugin",
            "options": {
              "dtoFileNameSuffix": [".ts", ".dto.ts"],
              "controllerFileNameSuffix": [".controller.ts"],
              "classValidatorShim": true,
              "dtoKeyOfComment": "description",
              "controllerKeyOfComment": "description",
              "introspectComments": true
            }
          }
        ]
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "assessment-result:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "assessment-result:build:development"
        },
        "production": {
          "buildTarget": "assessment-result:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/assessment-result/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/assessment-result/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "docker-build": {
      "dependsOn": ["build"],
      "command": "docker build -f apps/assessment-result/Dockerfile . -t thangnv0703/assessment-result-service & docker push thangnv0703/assessment-result-service"
    }
  },
  "tags": []
}
